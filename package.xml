<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>stem</name>
  <summary>a PHP extension that provides word stemming</summary>
  <description>This stem extension for PHP provides stemming capability for a variety of languages using Dr. M.F. Porter's Snowball API, which can be found at: http://snowball.tartarus.org</description>
  <maintainers>
    <maintainer>
      <user>jay</user>
      <name>Jay Smith</name>
      <email>jay@php.net</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>ohill</user>
      <name>Olivier Hill</name>
      <email>ohill@php.net</email>
      <role>helper</role>
    </maintainer>
  </maintainers>
  <release>
    <version>1.4.3</version>
    <date>2006-01-23</date>
    <license>BSD</license>
    <state>stable</state>
    <notes>Initial PECL release</notes>
    <deps>
      <dep type="php" rel="ge" version="4.3.0" optional="no"/>
    </deps>
    <configureoptions>
      <configureoption name="enable-stem-danish" default="yes" prompt="Compile Danish stemmer?"/>
      <configureoption name="enable-stem-dutch" default="yes" prompt="Compile Dutch stemmer?"/>
      <configureoption name="enable-stem-english" default="yes" prompt="Compile English stemmer?"/>
      <configureoption name="enable-stem-finnish" default="yes" prompt="Compile Finnish stemmer?"/>
      <configureoption name="enable-stem-french" default="yes" prompt="Compile French stemmer?"/>
      <configureoption name="enable-stem-german" default="yes" prompt="Compile German stemmer?"/>
      <configureoption name="enable-stem-italian" default="yes" prompt="Compile Italian stemmer?"/>
      <configureoption name="enable-stem-norwegian" default="yes" prompt="Compile Norwegian stemmer?"/>
      <configureoption name="enable-stem-portuguese" default="yes" prompt="Compile Portuguese stemmer?"/>
      <configureoption name="enable-stem-russian" default="yes" prompt="Compile Russian stemmer?"/>
      <configureoption name="enable-stem-russian_unicode" default="yes" prompt="Compile Russian (UTF8) stemmer?"/>
      <configureoption name="enable-stem-spanish" default="yes" prompt="Compile Spanish stemmer?"/>
      <configureoption name="enable-stem-swedish" default="yes" prompt="Compile Swedish stemmer?"/>
    </configureoptions>
  </release>
  <filelist>
    <file role="doc" name="COPYING" />
    <file role="doc" name="CREDITS" />
    <file role="doc" name="ChangeLog" />
    <file role="doc" name="EXPERIMENTAL" />
    <file role="doc" name="README" />
    <file role="doc" name="README.tests" />
    <file role="src" name="api.c" />
    <file role="src" name="api.h" />
    <file role="src" name="config.m4" />
    <file role="src" name="danish.c" />
    <file role="src" name="danish.h" />
    <file role="src" name="dutch.c" />
    <file role="src" name="dutch.h" />
    <file role="src" name="english.c" />
    <file role="src" name="english.h" />
    <file role="src" name="finnish.c" />
    <file role="src" name="finnish.h" />
    <file role="src" name="french.c" />
    <file role="src" name="french.h" />
    <file role="src" name="german.c" />
    <file role="src" name="german.h" />
    <file role="src" name="header.h" />
    <file role="src" name="italian.c" />
    <file role="src" name="italian.h" />
    <file role="src" name="norwegian.c" />
    <file role="src" name="norwegian.h" />
    <file role="src" name="php_stem.h" />
    <file role="src" name="porter.c" />
    <file role="src" name="porter.h" />
    <file role="src" name="portuguese.c" />
    <file role="src" name="portuguese.h" />
    <file role="src" name="russian.c" />
    <file role="src" name="russian.h" />
    <file role="src" name="russian_unicode.c" />
    <file role="src" name="russian_unicode.h" />
    <file role="src" name="spanish.c" />
    <file role="src" name="spanish.h" />
    <file role="src" name="stem.c" />
    <file role="src" name="swedish.c" />
